    	    }
  
        $("#menud").click(function (e) {
            e.stopPropagation(); 
            $(".menu").toggle(100);
        });
        
            $('#mycarouselExample').carousel({
                interval: 2000  ;
            });
            $('#carouselExample1').carousel({
                interval: 2000  ;
            });
          
     
    });
</script>
